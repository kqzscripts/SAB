UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout.Parent = ScrollFrame

local function createFeature(name, scriptSource)
    local Frame = Instance.new("Frame")
    Frame.Size = UDim2.new(1, -10, 0, 60)
    Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    Frame.BorderSizePixel = 0
    Frame.Parent = ScrollFrame
    
    local Corner = Instance.new("UICorner")
    Corner.CornerRadius = UDim.new(0, 6)
    Corner.Parent = Frame
    
    local Label = Instance.new("TextLabel")
    Label.Size = UDim2.new(1, -100, 1, 0)
    Label.Position = UDim2.new(0, 10, 0, 0)
    Label.BackgroundTransparency = 1
    Label.Text = name
    Label.TextColor3 = Color3.fromRGB(255, 255, 255)
    Label.TextSize = 16
    Label.Font = Enum.Font.GothamSemibold
    Label.TextXAlignment = Enum.TextXAlignment.Left
    Label.Parent = Frame
    
    local ActionFrame = Instance.new("Frame")
    ActionFrame.Size = UDim2.new(0, 180, 0, 40)
    ActionFrame.Position = UDim2.new(1, -190, 0.5, -20)
    ActionFrame.BackgroundTransparency = 1
    ActionFrame.Parent = Frame
    
    local CopyBtn = Instance.new("TextButton")
    CopyBtn.Size = UDim2.new(0, 85, 1, 0)
    CopyBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    CopyBtn.Text = "Copy"
    CopyBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    CopyBtn.Font = Enum.Font.Gotham
    CopyBtn.TextSize = 14
    CopyBtn.Parent = ActionFrame
    
    local CopyCorner = Instance.new("UICorner")
    CopyCorner.CornerRadius = UDim.new(0, 4)
    CopyCorner.Parent = CopyBtn
    
    local EjectBtn = Instance.new("TextButton")
    EjectBtn.Size = UDim2.new(0, 85, 1, 0)
    EjectBtn.Position = UDim2.new(0, 95, 0, 0)
    EjectBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    EjectBtn.Text = "Eject"
    EjectBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
    EjectBtn.Font = Enum.Font.GothamBold
    EjectBtn.TextSize = 14
    EjectBtn.Parent = ActionFrame
    
    local EjectCorner = Instance.new("UICorner")
    EjectCorner.CornerRadius = UDim.new(0, 4)
    EjectCorner.Parent = EjectBtn
    
    CopyBtn.MouseButton1Click:Connect(function()
        setclipboard(scriptSource)
        CopyBtn.Text = "Copied!"
        task.wait(2)
        CopyBtn.Text = "Copy"
    end)
    
    EjectBtn.MouseButton1Click:Connect(function()
        loadstring(scriptSource)()
        EjectBtn.Text = "Active!"
        task.wait(2)
        EjectBtn.Text = "Eject"
    end)
    
    ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y + 20)
end

-- Teleport Scripts with Anti-Cheat Bypasses
createFeature("VayziYT Hub (Insta TP)", [[loadstring(game:HttpGet("https://raw.githubusercontent.com/VayziYT/Steal-a-Brainrot-Rayfield/refs/heads/main/StealABrainrot"))()]])
createFeature("Chilli Hub (Multi-Farm)", [[loadstring(game:HttpGet('https://raw.githubusercontent.com/m00ndiety/Steal-a-brainrot/refs/heads/main/Steal-a-Brainrot'))()]])
createFeature("Advanced MOD TP", [[loadstring(game:HttpGet("https://raw.githubusercontent.com/gumanba/Scripts/refs/heads/main/StealaBrainrotMOD", true))()]])
createFeature("RedzHub (Auto Lock)", [[loadstring(game:HttpGet("https://raw.githubusercontent.com/REDZHUB/Steal-a-Brainrot/main/Script.lua"))()]])

-- Dragging Functionality
local dragging, dragInput, dragStart, startPos
TopBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)
