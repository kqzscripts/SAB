MainFrame.Size = UDim2.new(0, 340, 0, 120)
MainFrame.Position = UDim2.new(0.5, -170, 0.5, -60)
MainFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
MainFrame.BorderSizePixel = 0
MainFrame.Parent = TigyScriptUI
